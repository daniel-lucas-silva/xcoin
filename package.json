{
  "name": "xcoin",
  "version": "1.3.0",
  "description": "Cryptocurrency trading bot",
  "bugs": "https://github.com/rainfu/xcoin/issues",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/rainfu/xcoin.git"
  },
  "main": "./index.js",
  "bin": {
    "xcoin": "./xcoin.sh"
  },
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "lint": "eslint **/*.js",
    "lint-fix": "eslint **/*.js --fix",
    "precommit": "lint-staged",
    "exchange": "node xcoin.js exchange",
    "trade": "node xcoin.js trade",
    "server": "node xcoin.js server",
    "binanceusdm": "pm2 start ./data/pm2/binanceusdm.js",
    "binance": "pm2 start ./data/pm2/binance.js",
    "mexc": "pm2 start ./data/pm2/mexc.js",
    "xcoin": "pm2 start ./data/pm2/xcoin.config.js",
    "ciiat": "pm2 start ./data/pm2/ciiat.config.js",
    "uniswap": "pm2 start ./data/pm2/uniswap.config.js",
    "dev": "yarn trade mexc --conf ./data/config/mexc/30m.json --paper",
    "format": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\""
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.4.10",
    "@ethersproject/contracts": "^5.1.1",
    "@ethersproject/providers": "^5.4.4",
    "@ethersproject/units": "^5.1.0",
    "@pancakeswap/sdk": "^2.2.0",
    "@uniswap/sdk": "^3.0.3",
    "ccxt": "^4.3.77",
    "cheerio": "^1.0.0-rc.10",
    "colors": "^1.4.0",
    "commander": "^12.1.0",
    "cross-fetch": "^3.1.4",
    "ethers": "^5.1.4",
    "graphql": "^16.9.0",
    "https-proxy-agent": "^5.0.0",
    "jsbi": "^3.1.4",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "lolex": "^6.0.0",
    "minimist": "^1.2.5",
    "moment": "^2.30.1",
    "mongodb": "^6.8.0",
    "numbro": "^2.5.0",
    "pm2": "^5.4.2",
    "random-port": "^0.1.0",
    "technicalindicators": "^3.1.0",
    "timebucket": "^0.4.0",
    "web3": "^1.3.6",
    "ws": "^7.5.3",
    "zero-fill": "^2.2.4"
  },
  "devDependencies": {
    "@types/bun": "^1.1.6",
    "lint-staged": "^10.5.3",
    "prettier": "^3.3.3",
    "vitepress": "^1.0.0-alpha.65"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.2"
  },
  "snyk": true
}